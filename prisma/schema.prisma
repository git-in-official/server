generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Provider {
  google
  apple
}

model User {
  id         String   @id @default(uuid())
  provider   Provider
  providerId String
  name       String

  @@unique([provider, providerId])
}
